function xs(a,b){
    let c
    if(a==b){
        c=a;
    }
    else if(a>b){
        for(var i=1;i<b+1;i++){
            if(b%i==a%i){
                c=i;
            }
        }
    }
        else{
            for(var i=1;i<a+1;i++){
                if(a%i==b%i){
                    c=i;
                }
            }
    }
    return c
}
console.log('commonFactor(12,15)=',xs(12,15))
